{
  "private": true,
  "name": "excalidraw-monorepo",
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "excalidraw-app",
    "packages/*"
  ],
  "devDependencies": {
    "@babel/preset-env": "7.26.9",
    "@excalidraw/eslint-config": "1.0.3",
    "@excalidraw/prettier-config": "1.0.2",
    "@types/chai": "4.3.0",
    "@types/jest": "27.4.0",
    "@types/lodash.throttle": "4.1.7",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@types/socket.io-client": "3.0.0",
    "@vitejs/plugin-react": "3.1.0",
    "@vitest/coverage-v8": "3.0.7",
    "@vitest/ui": "2.0.5",
    "chai": "4.3.6",
    "dotenv": "16.0.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "3.3.1",
    "http-server": "14.1.1",
    "husky": "7.0.4",
    "jsdom": "22.1.0",
    "lint-staged": "12.3.7",
    "pepjs": "0.5.3",
    "prettier": "2.6.2",
    "rewire": "6.0.0",
    "rimraf": "^5.0.0",
    "typescript": "4.9.4",
    "vite": "5.0.12",
    "vite-plugin-checker": "0.7.2",
    "vite-plugin-ejs": "1.7.0",
    "vite-plugin-pwa": "0.21.1",
    "vite-plugin-svgr": "4.2.0",
    "vitest": "3.0.6",
    "vitest-canvas-mock": "0.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "homepage": ".",
  "prettier": "@excalidraw/prettier-config",
  "scripts": {
    "build:app": "npm --prefix ./excalidraw-app run build:app",
    "build:common": "npm --prefix ./packages/common run build:esm",
    "build:element": "npm --prefix ./packages/element run build:esm",
    "build:excalidraw": "npm --prefix ./packages/excalidraw run build:esm",
    "build:math": "npm --prefix ./packages/math run build:esm",
    "build:packages": "npm run build:common && npm run build:math && npm run build:element && npm run build:excalidraw",
    "build:version": "npm --prefix ./excalidraw-app run build:version",
    "build": "npm run build:packages && npm --prefix ./excalidraw-app run build",
    "start": "npm --prefix ./excalidraw-app run start",
    "clean": "rimraf --glob excalidraw-app/build excalidraw-app/dist excalidraw-app/dev-dist packages/*/dist packages/*/build"
  },
  "resolutions": {
    "strip-ansi": "6.0.1"
  }
}
